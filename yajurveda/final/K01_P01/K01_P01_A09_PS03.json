  "phrases": [
    {
      "text": "मौगररुस्ते दिवं मा स्कान्.",
      "start": 86.32,
      "end": 89.88
    },
    {
      "text": "वसवस्त्वा परि गृह्णन्तु गायत्रेण",
      "start": 89.88,
      "end": 94.24
    },
    {
      "text": "छन्दसारुद्रास्त्वा परि गृह्णन्तु त्रैष्टुभेन",
      "start": 94.24,
      "end": 99.32
    },
    {
      "text": "छन्दसाऽऽदित्यास्त्वा परि गृह्णन्तु जागतेन",
      "start": 99.32,
      "end": 104.6
    },
    {
      "text": "छन्दसा देवस्य सवितुः सवे",
      "start": 104.6,
      "end": 110.36
    },
    {
      "text": "कर्म कृण्वन्ति वेधस ऋतमस्यृतसदन-मस्यृतश्रीरसि",
      "start": 110.36,
      "end": 114.8
    },
    {
      "text": "धा असि स्वधा अस्युर्वी",
      "start": 114.8,
      "end": 117.92
    },
    {
      "text": "चासि वस्वी चासि पुरा",
      "start": 117.92,
      "end": 120.76
    },
    {
      "text": "क्रूरस्य विसृपो विरफ्शिन् (",
      "start": 120.76,
      "end": 123.88
    },
    {
      "text": ") नुदादाय पृथिवीं जीरदानुर्यामैरयन्",
      "start": 123.88,
      "end": 129.52
    },
    {
      "text": "चन्द्रमसि स्वधाभिस्तां धीरासो अनुदृश्य",
      "start": 129.52,
      "end": 135.32
    },
    {
      "text": "यजन्ते",
      "start": 135.32,
      "end": 136.52
    }

  "phrases": [
    {
      "text": "मयि धेहि वृत्रस्य कनीनिकाऽसि",
      "start": 31.8,
      "end": 45.52
    },
    {
      "text": "चक्षुष्पा असि चक्षुर्मे पाहि",
      "start": 45.52,
      "end": 50.72
    },
    {
      "text": "चित्पतिस्त्वा पुनातु वाक्पतिस्त्वा पुनातु",
      "start": 50.72,
      "end": 53.36
    },
    {
      "text": "देवस्त्वा सविता पुनात्वच्छिद्रेण पवित्रेण",
      "start": 53.36,
      "end": 59.52
    },
    {
      "text": "वसोः सूर्यस्य रश्मिभिस्तस्य ते",
      "start": 59.52,
      "end": 63.68
    },
    {
      "text": "पवित्रपते पवित्रेण यस्मै कं",
      "start": 63.68,
      "end": 67.04
    },
    {
      "text": "पुने तच्छकेयमा वो देवास",
      "start": 67.04,
      "end": 70.88
    },
    {
      "text": "ईमहे सत्यधर्माणो अद्ध्वरे यद्वो",
      "start": 70.88,
      "end": 75.48
    },
    {
      "text": "देवास आगुरे यज्ञियासो हवामह",
      "start": 75.48,
      "end": 80.08
    },
    {
      "text": "इन्द्राग्नी द्यावापृथिवी आप ओषधी",
      "start": 80.08,
      "end": 83.96
    },
    {
      "text": "( ) स्त्वं दीक्षाणा-मधिपतिरसीह",
      "start": 83.96,
      "end": 91.48
    },
    {
      "text": "मा सन्तं पाहि",
      "start": 91.48,
      "end": 93.92
    }
  ],
  "total_phrases": 12
}
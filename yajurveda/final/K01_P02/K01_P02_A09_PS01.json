  "phrases": [
    {
      "text": "प्र च्यवस्व भुवस्पते विश्वान्यभि",
      "start": 0.0,
      "end": 4.56
    },
    {
      "text": "धामानि मा त्वा परिपरी",
      "start": 4.56,
      "end": 8.24
    },
    {
      "text": "विदन्मा त्वा परिपन्थिनो विदन्मा",
      "start": 8.24,
      "end": 11.8
    },
    {
      "text": "त्वा वृका अघायवो मा",
      "start": 11.8,
      "end": 14.56
    },
    {
      "text": "गन्धर्वो विश्वावसुरा दघच्छ्येनो भूत्वा",
      "start": 14.56,
      "end": 18.72
    },
    {
      "text": "परा पत यजमानस्य नो",
      "start": 18.72,
      "end": 20.64
    },
    {
      "text": "गृहे देवैः सꣳस्कृतं ॅयजमानस्य",
      "start": 20.64,
      "end": 25.72
    },
    {
      "text": "स्वस्त्ययन्यस्यपि पन्थामगस्महि स्वस्तिगा-मनेहसं ॅयेन",
      "start": 25.72,
      "end": 31.8
    },
    {
      "text": "विश्वाः परि द्विषो वृणक्ति",
      "start": 31.8,
      "end": 35.08
    },
    {
      "text": "विन्दते वसु नमो मित्रस्य",
      "start": 35.08,
      "end": 37.4
    },
    {
      "text": "( ) वरुणस्य चक्षसे",
      "start": 37.4,
      "end": 39.2
    },
    {
      "text": "महो देवाय तदृतꣳ सपर्यत",
      "start": 39.2,
      "end": 41.68
    },
    {
      "text": "दूरेदृशे देवजाताय केतवे दिवस्पुत्राय",
      "start": 41.68,
      "end": 47.64
    },
    {
      "text": "सूर्याय शꣳसत वरुणस्य स्कंभनमसि",
      "start": 47.64,
      "end": 52.04
    },
    {
      "text": "वरुणस्य स्कंभसर्जन-मस्युन्मुक्तो वरुणस्य पाशः",
      "start": 52.04,
      "end": 57.88
    }
  ],
  "total_phrases": 15
}
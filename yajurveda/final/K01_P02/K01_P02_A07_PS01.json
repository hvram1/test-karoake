  "phrases": [
    {
      "text": "सोमं ते क्रीणाम्यूर्जस्वन्तं पयस्वन्तं",
      "start": 0.0,
      "end": 6.0
    },
    {
      "text": "ॅवीर्यावन्तमभि-मातिषाहꣳ शुक्रं ते शुक्रेण",
      "start": 6.0,
      "end": 11.56
    },
    {
      "text": "क्रीणामि चन्द्रं चन्द्रेणामृतममृतेन सम्यत्ते",
      "start": 11.56,
      "end": 16.96
    },
    {
      "text": "गोरस्मे चन्द्राणि तपसस्तनूरसि प्रजापतेर्",
      "start": 16.96,
      "end": 22.44
    },
    {
      "text": "वर्णस्तस्यास्ते सहस्रपोषं पुष्यन्त्याश्चरमेण पशुना",
      "start": 22.44,
      "end": 30.2
    },
    {
      "text": "क्रीणाम्यस्मे ते बन्धुर्मयि ते",
      "start": 30.2,
      "end": 34.08
    },
    {
      "text": "रायः श्रयन्तामस्मे ज्योतिः सोमविक्रयिणि",
      "start": 34.08,
      "end": 39.24
    },
    {
      "text": "तमो मित्रो न एहि",
      "start": 39.24,
      "end": 41.88
    },
    {
      "text": "सुमित्रधा इन्द्रस्योरु ( )",
      "start": 41.88,
      "end": 43.92
    },
    {
      "text": "मा विश दक्षिण-मुशन्नुशन्तꣳ स्योनः",
      "start": 43.92,
      "end": 48.84
    },
    {
      "text": "स्योनꣳ स्वान भ्राजाङ्घारे बंभारे",
      "start": 48.84,
      "end": 52.76
    },
    {
      "text": "हस्त सुहस्त कृशानवेते वः",
      "start": 52.76,
      "end": 55.96
    },
    {
      "text": "सोम क्रयणास्तान् रक्षद्ध्वं मा",
      "start": 55.96,
      "end": 60.76
    },
    {
      "text": "वो दभन्",
      "start": 60.76,
      "end": 61.56
    }
  ],
  "total_phrases": 14
}